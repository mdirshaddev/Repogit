{
  "name": "repogit",
  "homepage": "https://mdirshaddev.github.io/repogit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdirshaddev/repogit.git"
  },
  "bugs": {
    "url": "https://github.com/mdirshaddev/repogit/issues"
  },
  "version": "1.0.0",
  "description": "This is a Static Github Api based react app",
  "main": "index.js",
  "scripts": {
    "server-start": "webpack serve --config webpack/webpack.config.js --env env=development --progress",
    "build": "webpack --config webpack/webpack.config.js --env env=production",
    "build:analyze": "npm run build -- --env addon=bundleanalyze",
    "build:clean": "rm -rf ./build",
    "serve": "serve -s build",
    "serve:analyze": "serve -s build/report.html",
    "lint": "eslint ./",
    "install-client": "npm install --save-dev",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "cd backend && npm install && node server.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "keywords": [
    "Webpack5",
    "React",
    "Redux",
    "Jest"
  ],
  "author": "Md Irshad",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-react-constant-elements": "^7.12.13",
    "@babel/plugin-transform-react-inline-elements": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@storybook/storybook-deployer": "^2.8.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.0",
    "mini-css-extract-plugin": "^1.3.5",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.3",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.7.3",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.1.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "node-sass": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-lazyload": "^3.2.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1"
  }
}
